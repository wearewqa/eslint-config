{
  "name": "@wearewqa/eslint-config",
  "version": "1.0.16",
  "description": "ESLint and Prettier Config",
  "keywords": [
    "javascript",
    "typescript",
    "ecmascript",
    "eslint",
    "eslint-config",
    "lint",
    "config",
    "prettier"
  ],
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/wearewqa/eslint-config.git"
  },
  "author": "Matt Burns <matt.burns@wearewqa.com>",
  "license": "MIT",
  "peerDependencies": {
    "eslint": "^8.0.0",
    "prettier": ">=3.0.0",
    "typescript": "^4.8.4 || ^5.0.0 || ^5.5.0",
    "@typescript-eslint/eslint-plugin": "^6.10.0"
  },
  "eslintConfig": {
    "extends": [
      "./.eslintrc.js"
    ]
  },
  "dependencies": {
    "@babel/core": "^7.23.3",
    "@babel/eslint-parser": "^7.23.3",
    "@babel/preset-react": "^7.23.3",
    "@rushstack/eslint-patch": "^1.5.1",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-html": "^7.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^5.0.0"
  },
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint --fix",
    "bump:beta": "npm version prerelease --preid beta",
    "publish:beta": "npm publish --tag beta",
    "release": "npm version patch --force && npm publish"
  }
}
